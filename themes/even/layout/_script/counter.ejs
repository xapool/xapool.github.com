<% if (theme.leancloud && theme.leancloud.app_id && theme.leancloud.app_key) { %>
<script src="//unpkg.com/leancloud-storage@4.12.2/dist/av-min.js"></script>
<script id="leancloud">
  AV.init({
    appId: "<%- theme.leancloud.app_id %>",
    appKey: "<%- theme.leancloud.app_key %>"
  });
</script>
<% } %>
